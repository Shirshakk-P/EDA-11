      "source": [
        "class NRELSolarDataClient:\n",
        "    def __init__(self, api_key):\n",
        "        self.api_key = api_key\n",
        "        self.base_url = \"https://developer.nrel.gov/api\"\n",
        "        self.rate_limit_delay = 3.6\n",
        "\n",
        "    def get_solar_resource_data(self, latitude, longitude, format='json'):\n",
        "        url = f\"{self.base_url}/solar/solar_resource/v1.{format}\"\n",
        "        params = {\n",
        "            'api_key': self.api_key,\n",
        "            'lat': latitude,\n",
        "            'lon': longitude\n",
        "        }\n",
        "        try:\n",
        "            response = requests.get(url, params=params)\n",
        "            response.raise_for_status()\n",
        "            time.sleep(self.rate_limit_delay)\n",
        "            try:\n",
        "                return response.json()\n",
        "            except Exception:\n",
        "                return response.text\n",
        "        except requests.exceptions.RequestException as e:\n",
        "            logger.error(f\"Error fetching solar data for {latitude}, {longitude}: {e}\")\n",
        "            return None\n",
        "\n",
        "    def batch_solar_data_collection(self, locations_df, save_path='data/raw/nrel_solar_data.csv'):\n",
        "        results = []\n",
        "        total_locations = len(locations_df)\n",
        "        for idx, row in locations_df.iterrows():\n",
        "            logger.info(f\"Processing {idx+1}/{total_locations}: {row['country']}, {row['region']}\")\n",
        "            solar_data = self.get_solar_resource_data(row['lat'], row['lon'])\n",
        "            logger.info(f\"RAW solar_data for {row['country']}, {row['region']}: {solar_data}\")\n",
        "\n",
        "            # Parse if API returned string\n",
        "            if isinstance(solar_data, str):\n",
        "                try:\n",
        "                    solar_data = json.loads(solar_data)\n",
        "                except Exception:\n",
        "                    logger.error(f\"Failed to parse solar_data for {row['country']}, {row['region']}: {solar_data}\")\n",
        "                    continue\n",
        "\n",
        "            if not (solar_data and isinstance(solar_data, dict) and 'outputs' in solar_data):\n",
        "                logger.error(f\"Invalid or missing 'outputs' in solar_data for {row['country']}, {row['region']}: {solar_data}\")\n",
        "                continue\n",
        "\n",
        "            outputs = solar_data['outputs']\n",
        "\n",
        "            if (\n",
        "                outputs.get('avg_dni') == 'no data' or\n",
        "                outputs.get('avg_ghi') == 'no data' or\n",
        "                outputs.get('avg_lat_tilt') == 'no data'\n",
        "            ):\n",
        "                logger.warning(f\"No solar resource data for {row['country']}, {row['region']} at ({row['lat']}, {row['lon']})\")\n",
        "                continue\n",
        "\n",
        "            try:\n",
        "                result = {\n",
        "                    'country': row['country'],\n",
        "                    'region': row['region'],\n",
        "                    'latitude': row['lat'],\n",
        "                    'longitude': row['lon'],\n",
        "                    'annual_dni': outputs['avg_dni'],\n",
        "                    'annual_ghi': outputs['avg_ghi'],\n",
        "                    'annual_tilt': outputs['avg_lat_tilt'],\n",
        "                    'collection_date': datetime.now().isoformat()\n",
        "                }\n",
        "                # Monthly data extractions\n",
        "                if isinstance(outputs['avg_ghi'], dict):\n",
        "                    for month, val in outputs['avg_ghi']['monthly'].items():\n",
        "                        result[f'ghi_month_{month}'] = val\n",
        "                if isinstance(outputs['avg_dni'], dict):\n",
        "                    for month, val in outputs['avg_dni']['monthly'].items():\n",
        "                        result[f'dni_month_{month}'] = val\n",
        "                results.append(result)\n",
        "            except Exception as e:\n",
        "                logger.error(f\"Error processing solar data for {row['country']}, {row['region']}: {e}\")\n",
        "                continue\n",
        "\n",
        "        df = pd.DataFrame(results)\n",
        "        os.makedirs(os.path.dirname(save_path), exist_ok=True)\n",
        "        df.to_csv(save_path, index=False)\n",
        "        logger.info(f\"Saved NREL solar data for {len(results)} locations to {save_path}\")\n",
        "        return df"
      ]
    },
