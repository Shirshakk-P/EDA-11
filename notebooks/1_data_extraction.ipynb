        "class IRENADataExtractor:\n",
        "    def __init__(self, irena_csv_path):\n",
        "        self.irena_csv_path = irena_csv_path\n",
        "\n",
        "    def load_irena_data(self):\n",
        "        logger.info(f\"Loading IRENA data from {self.irena_csv_path}\")\n",
        "        df = pd.read_csv(self.irena_csv_path, skiprows=1)\n",
        "        df.columns = [col.strip().replace('\"', '') for col in df.columns]\n",
        "        df.rename(columns={\n",
        "            'Region/country/area': 'country',\n",
        "            'Indicator': 'indicator',\n",
        "            'Year': 'year',\n",
        "            'Renewable energy share of electricity capacity and generation (%)': 're_share_percent'\n",
        "        }, inplace=True)\n",
        "        df['year'] = df['year'].astype(int)\n",
        "        logger.info(f\"Loaded IRENA data shape: {df.shape}\")\n",
        "        return df"
      ]
    },
