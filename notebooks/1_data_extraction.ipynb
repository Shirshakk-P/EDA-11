        "import rasterio\n",
        "import pandas as pd\n",
        "from rasterio.enums import Resampling\n",
        "from rasterio.warp import transform\n",
        "from shapely.geometry import Point\n",
        "\n",
        "def extract_wind_at_locations(raster_path, locations_df, lat_col='lat', lon_col='lon', new_column='wind_speed'):\n",
        "    with rasterio.open(raster_path) as src:\n",
        "        # Transform lat/lon to raster CRS\n",
        "        raster_crs = src.crs\n",
        "        lats = locations_df[lat_col].values\n",
        "        lons = locations_df[lon_col].values\n",
        "\n",
        "        xs, ys = rasterio.warp.transform({'init': 'EPSG:4326'}, raster_crs, lons, lats)\n",
        "\n",
        "        # Extract raster values at each location\n",
        "        values = []\n",
        "        for x, y in zip(xs, ys):\n",
        "            row, col = src.index(x, y)\n",
        "            try:\n",
        "                val = src.read(1)[row, col]\n",
        "            except IndexError:\n",
        "                val = None\n",
        "            values.append(val)\n",
        "\n",
        "    locations_df[new_column] = values\n",
        "    return locations_df"
      ]
    },
